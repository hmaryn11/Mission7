
@{ ViewData["Title"] = "Delete Entry"
}
@model EntryResponse
<h2>Confirm Deletion</h2>
<h4>Are you sure you want to delete the record for @Model.movieTitle?</h4>

<form method="post" asp-action="Delete">
    <button type="submit">Delete</button>
    <a class="btn btn-primary" asp-controller="Home" asp-action="MovieTable">Cancel</a>
</form>
